<% if user_signed_in? %>
    <% if current_user.role == "ADMIN" %>
      <%= react_component("orders/Index", {signed: true, current_user: current_user, orders: @orders_json}) %>
    <% else %>
        <% controller.redirect_back_or_to '/' %>
    <% end %>
<% else %>
    <%= controller.redirect_back_or_to '/' %>
<% end%>
