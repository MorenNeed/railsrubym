<% if user_signed_in? %>
    <% if current_user.role == "ADMIN" %>
      <%= react_component("items/Manage", {signed: true, current_user: current_user, items: @items}) %>
    <% else %>
        <% controller.redirect_back_or_to '/' %>
    <% end %>
<% else %>
    <%= controller.redirect_back_or_to '/' %>
<% end%>
